Private Sub CmdIO_Click(Index As Integer)
Dim i, j As Integer
Dim strtmp As String
If Index = 0 Then
    If IsNull(mvarForm) = False Then
  '  If Not mvarForm = Nothing Then
        Unload mvarForm
    End If
End If
If Index = 1 Then
    For j = 1 To Cln.Count
        For i = 1 To Cln.Count
           If Cln(i).Priority = j Then
                If Text1((j - 1) * 2).Visible Then
                    If Text1((j - 1) * 2).Text <> "" Then
                        Cln(i).MinValue = Text1((j - 1) * 2).Text
                    Else
                        Cln(i).MinValue = Cln(i).DefaultMin
                    End If
                    If Cln(i).DataType = "String" Then
                        Cln(i).MinValue = "'" & Cln(i).MinValue & "'"
                    ElseIf Cln(i).DataType = "Date" Then
                        Cln(i).MinValue = "[" & Cln(i).MinValue & "]"
                    End If
                    If Cln(i).Num = 1 Then
                        Sqlstring = Sqlstring & Cln(i).FieldName & " = " & Cln(i).MinValue & "  And "
                    ElseIf Cln(i).Num = 2 Then
                        Sqlstring = Sqlstring & Cln(i).FieldName & " >= " & Cln(i).MinValue & "  And "
                    End If
                End If
                If Text1((j - 1) * 2 + 1).Visible Then
                    If Text1((j - 1) * 2 + 1).Text <> "" Then
                        Cln(i).MaxValue = Text1((j - 1) * 2 + 1).Text
                    Else
                        Cln(i).MaxValue = Cln(i).DefaultMax
                    End If
                    If Cln(i).DataType = "String" Then
                        Cln(i).MaxValue = "'" & Cln(i).MaxValue & "'"
                    ElseIf Cln(i).DataType = "Date" Then
