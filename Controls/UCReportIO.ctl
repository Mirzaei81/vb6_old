Attribute VB_Name = "UCReportIO"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Explicit
Public DCol As New Collection
Private Cln As New Collection
Private mvarCmdCount As Integer 'local copy
Private mvarTxtCount As Integer 'local copy
Private mvarMskCount As Integer 'local copy
Private mvarResultCommand As Integer
Private mvarResultText(3) As String
Private mvarNoMsg As Boolean
Private mvarForm As Form
'Private lblIO(17) As Label
Private TXTIO(26) As TextBox
Private cmbIO(26) As ComboBox
Private mskIO(26) As MaskEdBox
'Private CmdIO(2) As CommandButton
Private mvarIndex As Integer
Private mvarFinalSql As String
Private Sqlstring  As String
Private cls As ClnReportBase
Private keytxt As New Collection
Event Commandclick(Index As Integer)
Private Conn As New ADODB.Connection
Private Rc As New ADODB.Recordset
Private ClsGl As New ClsGl
Public Enum TypeObject
    vbtextbox = 1
    vbcombobox = 2
    vbmaskbox = 3
End Enum
Public Enum AlignmentObject
    LeftToRight = 1
    RightToLeft = 2
End Enum


Public Function Add(obj As TypeObject, Num As Integer, FieldName As String, Key As String, Optional DefaultMin As Variant, Optional DefaultMax As Variant, Optional FieldCode As String, Optional FieldDscr As String, Optional rctmp As String, Optional AlignObJ As AlignmentObject) As ClsReportBase
    Dim strCnn As String
    Dim i As Integer
    Dim KeyExist As Boolean
    Dim ObjNewMember As ClsReportBase
    On Error Resume Next
    Set ObjNewMember = New ClsReportBase
    Set Cln = mcol
    'set the properties passed into the method
    ObjNewMember.Num = Num
    ObjNewMember.FieldName = FieldName
    ObjNewMember.Key = Key
    ObjNewMember.DefaultMin = DefaultMin
    ObjNewMember.DefaultMax = DefaultMax
    ObjNewMember.FieldCode = FieldCode
    ObjNewMember.FieldDscr = FieldDscr
    ObjNewMember.rctmp = rctmp
    If Len(Key) = 0 Then
        DCol.Add ObjNewMember
    Else
        DCol.Add ObjNewMember, Key
    End If
    KeyExist = False
    For i = 1 To Cln.Count
        If Key = Cln(i).Key Then
            KeyExist = True
            Exit For
        End If
    Next i
    If KeyExist Then
        Cln(Key).Num = Num
        Cln(Key).FieldName = FieldName
        Cln(Key).Priority = mvarIndex + 1
        Cln(Key).DefaultMin = DefaultMin
        Cln(Key).DefaultMax = DefaultMax
        If obj = vbcombobox Then
            Cln(Key).FieldCode = FieldCode
            Cln(Key).FieldDscr = FieldDscr
            Cln(Key).rctmp = rctmp
        End If
        Select Case Num
            Case 1:
                If obj = vbtextbox Then
                    Text1(mvarIndex * 2).Visible = True
                    If AlignObJ = LeftToRight Then
                        Text1(mvarIndex * 2).Alignment = 0
                        Text1(mvarIndex * 2).RightToLeft = False
                    End If
                ElseIf obj = vbmaskbox Then
                    MaskEdBox1(mvarIndex * 2).Visible = True
                
                ElseIf obj = vbcombobox Then
                    Combo1(mvarIndex * 2).Visible = True
                End If
                LblIo(mvarIndex * 2).Visible = True
                LblIo(mvarIndex * 2).Caption = Cln(Key).DscrFull
                If UCase(Key) = "DAT" Then
                    keytxt.Add mvarIndex * 2
                    Text1(mvarIndex * 2).Alignment = 1
                    Text1(mvarIndex * 2).RightToLeft = False
                End If
            Case 2:
                If obj = vbtextbox Then
                    Text1(mvarIndex * 2).Visible = True
                    If AlignObJ = LeftToRight Then
                        Text1(mvarIndex * 2).Alignment = 0
                        Text1(mvarIndex * 2).RightToLeft = False
                    End If
                ElseIf obj = vbmaskbox Then
                    MaskEdBox1(mvarIndex * 2).Visible = True
                
                ElseIf obj = vbcombobox Then
                    Combo1(mvarIndex * 2).Visible = True
                End If
                LblIo(mvarIndex * 2).Visible = True
                LblIo(mvarIndex * 2).Caption = Cln(Key).DscrIn
                If UCase(Key) = "DAT" Then
                    keytxt.Add mvarIndex * 2
                    frmRep.FarDate1.Visible = True
                    frmRep.FarDate2.Visible = True
                    Text1(mvarIndex * 2).Alignment = 1
                    Text1(mvarIndex * 2).RightToLeft = False
                End If
                If obj = vbtextbox Then
                    Text1(mvarIndex * 2 + 1).Visible = True
                    If AlignObJ = LeftToRight Then
                        Text1(mvarIndex * 2 + 1).Alignment = 0
                        Text1(mvarIndex * 2 + 1).RightToLeft = False
                    End If
                ElseIf obj = vbmaskbox Then
                    MaskEdBox1((mvarIndex * 2) + 1).Visible = True
                
                ElseIf obj = vbcombobox Then
                    Combo1(mvarIndex * 2 + 1).Visible = True
                End If
                LblIo(mvarIndex * 2 + 1).Visible = True
                LblIo(mvarIndex * 2 + 1).Caption = Cln(Key).DscrOut
                If UCase(Key) = "DAT" Then
                    keytxt.Add mvarIndex * 2 + 1
                    Text1(mvarIndex * 2 + 1).Alignment = 1
                    Text1(mvarIndex * 2 + 1).RightToLeft = False
                End If
        End Select
        If obj = vbcombobox Then
            
            Conn.ConnectionString = strConnectionString
            If Conn.State <> 1 Then Conn.Open
            
            With Rc
                .ActiveConnection = Conn
                .CursorLocation = adUseClient
                .CursorType = adOpenStatic
                .LockType = adLockOptimistic
                .Source = rctmp
                .Open
            End With
            Select Case Num
            Case 1:
                ClsGl.FillList Combo1(mvarIndex * 2), Rc, FieldCode, FieldDscr
                Combo1(mvarIndex * 2).ListIndex = 0
            Case 2:
                ClsGl.FillList Combo1(mvarIndex * 2), Rc, FieldCode, FieldDscr
                ClsGl.FillList Combo1(mvarIndex * 2 + 1), Rc, FieldCode, FieldDscr
                Combo1(mvarIndex * 2).ListIndex = 0
                Combo1(mvarIndex * 2 + 1).ListIndex = Combo1(mvarIndex * 2 + 1).ListCount - 1
            End Select
            Rc.Close
            If Conn.State <> 0 Then Conn.Close
        End If
        mvarIndex = mvarIndex + 1
    End If
    Set Add = ObjNewMember
    Set ObjNewMember = Nothing
End Function

Public Property Get Item(vntIndexKey As Variant) As ClsReportBase
    'used when referencing an element in the collection
    'vntIndexKey contains either the Index or Key to the collection,
    'this is why it is declared as a Variant
    'Syntax: Set foo = x.Item(xyz) or Set foo = x.Item(5)
  Set Item = DCol(vntIndexKey)
