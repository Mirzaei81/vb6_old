Private Sub FillChildMenu(parentId As Integer, parentName As String)
    Dim Result As Boolean
    On Error GoTo ErrHandler
    Dim Img
    trMenu.ImageList = ImageList2
        For Each Img In ImageList2.ListImages
            If Img.Key = CStr(parentId) & "n" Then
                Result = True
                Exit For
            End If
        Next
        If Result Then
            trMenu.Nodes.Add , , CStr(parentId) & "n", parentName, CStr(parentId) & "n"
        Else
            trMenu.Nodes.Add , , CStr(parentId) & "n", parentName
        End If
        If parentId < 470 Then
        ReDim Parameter(2)
        Parameter(0) = GenerateInputParameter("@ObjectParent", adInteger, 4, parentId)
        Parameter(1) = GenerateInputParameter("@intAccessLevel", adInteger, 4, mVarAccessLevel)
        Parameter(2) = GenerateInputParameter("@intLanguage", adInteger, 4, clsStation.Language)
        
        Set Rst = RunParametricStoredProcedure2Rec("Get_ChildObject", Parameter)
        
        If Not (Rst.EOF And Rst.BOF) Then
            Do While Rst.EOF = False
                If Rst!intObjectCode <> 208 And Rst!intObjectCode <> 209 And Rst!intObjectCode <> 279 And Rst!intObjectCode <> 308 And Rst!intObjectCode <> 310 And Rst!intObjectCode <> 311 And Rst!intObjectCode <> 325 And Rst!intObjectCode <> 343 Then
                    Result = False
                    For Each Img In ImageList2.ListImages
                        If Img.Key = CStr(Rst!intObjectCode) & "n" Then
                            Result = True
                            Exit For
                        End If
                    Next
                    If Result Then
                        trMenu.Nodes.Add parentId & "n", tvwChild, Rst!intObjectCode & "n", Rst!ObjectName, CStr(Rst!intObjectCode) & "n"
                    Else
                        trMenu.Nodes.Add parentId & "n", tvwChild, Rst!intObjectCode & "n", Rst!ObjectName
                       ' trMenu.Nodes.Item(1).
                    End If
                End If
                Rst.MoveNext
            Loop
        End If
        End If
