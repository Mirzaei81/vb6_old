Private Sub ShowClickForm(Key As String)
    
    Dim Addad1 As Long
    Dim Addad2 As Long
    Dim Answer As Boolean
    Dim Result As Boolean
    Dim frmActive As Form
    Dim strFormName As String
    Dim strObjectCode As String
    strObjectCode = Key  'node.Key
''    Dim AllMenuClose As Boolean
''    Dim varNode As node
''    AllMenuClose = True
''    For Each node In trMenu.Nodes
''        Set varNode = node.Child
''        If node.Expanded = True And node.Children > 1 Then
''            AllMenuClose = False
''            varNode.EnsureVisible
''            Exit For
'''            For i = 1 To node.Children
'''               varNode.EnsureVisible
'''               Set varNode = varNode.Next
'''            Next i
''        End If
''    Next '    Node.EnsureVisible
''    If AllMenuClose = True Then trMenu.Nodes(1).EnsureVisible
''    Set varNode = Nothing

    If strObjectCode = "208n" Then Exit Sub  ''   
    If strObjectCode = "101n" Or strObjectCode = "102n" Or strObjectCode = "103n" Or strObjectCode = "104n" Or _
        strObjectCode = "105n" Or strObjectCode = "106n" Or strObjectCode = "107n" Or strObjectCode = "126n" Or _
        strObjectCode = "279n" Or strObjectCode = "325n" Or strObjectCode = "483n" Or strObjectCode = "488n" Or strObjectCode = "495n" Or _
        strObjectCode = "502n" Or strObjectCode = "508n" Then Exit Sub           ''   
    
    strObjectCode = Mid(strObjectCode, 1, Len(strObjectCode) - 1)
    
    On Error GoTo ErrHandler
    Result = False
    ReDim Parameter(0)
    Parameter(0) = GenerateInputParameter("Get_ObjectInfo", adInteger, 4, CInt(strObjectCode))
    
    Set Rst = RunParametricStoredProcedure2Rec("Get_ObjectInfo", Parameter)
    If Not (Rst.EOF And Rst.BOF) Then
        strFormName = Rst!ObjectId
        If LCase(Rst!ObjectId) = "frmreports" Then strFormName = "frmReportsItem"
    End If
    For Each varForm In Forms
        Debug.Print varForm.Name
        If LCase(strFormName) = LCase(varForm.Name) Then
            Result = True
            Exit For
        End If
    Next
     If Result = False Then
        If CInt(strObjectCode) = 343 And clsArya.ExternalAccounting = False And HasMiniAcc = True Then
            Exit Sub
        ElseIf CInt(strObjectCode) = 470 And clsArya.ExternalAccounting = False And HasMiniAcc = True Then
            Exit Sub
        ElseIf CInt(strObjectCode) = 343 Then
            If ClsFormAccess.frmAccount = True Then
                ShowAccountingForm "frmAccount", "  ": Exit Sub         'CInt(strObjectCode)
            Else
                ShowDisMessage "       ", 1500
            End If
        ElseIf CInt(strObjectCode) = 527 Then
            Accounting.SaresidState 1: ShowAccountingForm "frmSarresid", " ": Exit Sub         'CInt(strObjectCode)
        ElseIf CInt(strObjectCode) = 528 Then
            Accounting.SaresidState 2: ShowAccountingForm "frmSarresid", " ": Exit Sub         'CInt(strObjectCode)
        ElseIf CInt(strObjectCode) = 487 Then
            Accounting.ReceivedVar 1, 0, "", 0, 0, False, 0, "", 0
            ShowAccountingForm "frmReceive", "    "
        ElseIf CInt(strObjectCode) = 504 Then
            Accounting.PaymentVar 1, 0, "", 0, 0, False, 0, "", 0
            ShowAccountingForm "frmPayment", "    "
        ElseIf CInt(strObjectCode) = 484 Then
            ShowAccountingForm "frmTurnOver", "     "
        ElseIf CInt(strObjectCode) = 480 Then
            ShowAccountingForm "frmAcc_Bank", "    "
        ElseIf CInt(strObjectCode) = 481 Then
            ShowAccountingForm "frmBankAccounts", "    "
        ElseIf CInt(strObjectCode) = 482 Then
            ShowAccountingForm "frmcheckBook", "    "
        ElseIf CInt(strObjectCode) = 540 Then
            If ClsFormAccess.frmTarazSoodZian = True Then
                ShowAccountingForm "frmTaraz", "  "
            Else
                ShowDisMessage "       ", 1500
            End If
        ElseIf CInt(strObjectCode) = 542 Then
            If ClsFormAccess.SalarySystem = True Then
                ShowAccountingForm "frmSalary", "   ": Exit Sub         'CInt(strObjectCode)
            Else
                ShowDisMessage "       ", 1500
            End If
        ElseIf LCase(strFormName) = "frminvoice" Then
            frmInvoice.Show
        ElseIf LCase(strFormName) = "frmpurchase" Then
            frmPurchase.Show
        ElseIf LCase(strFormName) = "frmsms" Then
            If HasAryaSms = True Then
                Dim AryaSms
                Set AryaSms = CreateObject("AryaSms.clsMonitoring")
                
                AryaSms.SendVariables strMainKey, clsArya.ServerName, clsArya.DbName, clsArya.DBLogin, clsArya.DBPass, Trim(clsArya.Company), "lemon74300347", App.Path
                AryaSms.ShowForms
            Else
                ShowDisMessage "           ", 1500
            End If
        ElseIf LCase(strFormName) = "frmttms" Then
            If HasTTMS = True Then
                Dim clsttms As AryaTTMS.clsttms
                Set clsttms = New AryaTTMS.clsttms
                'clsdiscount.SetSMSDatabase "ServerName", "DbName", "DBLogin", "DBPass"
                clsttms.SetDatabase clsArya.ServerName, clsArya.DbName, clsArya.DBLogin, SqlPass
                
                Addad1 = 5000
                Addad2 = Math.Round(Addad1 * 56.365 + 1325570, 0)
                Answer = clsttms.InitFunc(Addad1, Addad2, KarbarKey, Server_IP, clsArya.NetLock)
            Else
                ShowDisMessage "           ", 1500
            End If
            
        ElseIf LCase(strFormName) = LCase("frmExcellTransfer") Then
            If HasExcell = True Then
                Dim clsexcel As AryaExcelTransfer.clsexcel
                Set clsexcel = New AryaExcelTransfer.clsexcel
                Addad1 = 5200
                Addad2 = Math.Round(Addad1 * 56.365 + 1325570, 0)
                Answer = clsexcel.InitFunc(Addad1, Addad2)
            Else
                ShowDisMessage "           ", 1500
            End If
            
        Else
            Set frmActive = Forms.Add(strFormName)
            frmActive.Show
            Set frmActive = Nothing
        End If
'        Dim nodX
'        For Each nodX In trMenu.Nodes
'            nodX.Expanded = False
'           ' nodX.EnsureVisible
'        Next nodX
    End If
     
    
    Exit Sub
ErrHandler:
    If err.Description <> "Object was unloaded" Then
        ShowDisMessage err.Description, 1000
        LogSave "frmGroupMenu", err, "trMenu_NodeClick"
    End If
End Sub

